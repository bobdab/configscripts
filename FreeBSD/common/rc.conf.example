# Auto-Enabled NICs from pc-sysinstall

wlans_ath0="wlan0"

ifconfig_wlan0="WPA SYNCDHCP"

# Auto-Enabled NICs from pc-sysinstall

hostname="L250"

pcsysconfig_enable="YES"

zfs_enable="YES"

# Enable OpenNTPD

ntpd_enable="NO"

openntpd_enable="YES"

vboxnet_enable="YES"

sshd_enable="YES"

pf_rules="/etc/pf.conf"

hald_enable="YES"
dbus_enable="YES"

# nfs client:
nfs_client_enable="YES"
# nfs server
portmap_enable="YES"
rpcbind_enable="YES"
rpc_lockd_enable="YES"
rpc_statd_enable="YES"
nfs_server_enable="YES"
mountd_enable="YES"
mountd_flags="-r -n"
nfsd_flags="-t -u -h 192.168.1.6"
